model EmTest

type int
type string

external VerifyString using string
external VerifyPassword using string

entity Thing {
	MadeOf: string
} 

entity Person : Thing {
	Cpr: int
} 

entity User : Person {
    DbId: int
    Name: string require VerifyString
    Password: string require VerifyPassword
    Age: int require Age > 0 && Age < 130
}

entity Resource1 {
	kind: string
	age: int
}

controller UserController uses User {
    CREATE with Resource1:kind exclude DbId
    GET on Name
    UPDATE on DbId with Password, Name
    LIST
}